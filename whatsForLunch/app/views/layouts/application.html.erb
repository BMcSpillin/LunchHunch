<!DOCTYPE html>
<html>
<head>
  <title>What's For Lunch?</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <link rel="stylesheet" type="text/css" href="/assets/main.css">

  <%#= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['GOOGLE_MAPS']}&sensor=true" %>

</head>
<body>
	<br />
	<div id="app"><img id="phone" src="/assets/phone.jpg"></div>
	<div id="content-box">
		<div id="content">
			<div id="header"></div>
			<div id="body">
				<img id="logo" src="/assets/logo.jpg" width="300">
				<%= yield %>
			</div>
			<div id="footer"></div>
		</div>
	</div>
</body>
</html>